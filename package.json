{
  "name": "dibbz_server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon",
    "initKnex": "knex init",
    "createmigrate": "knex migrate:make migrate",
    "migrate": "knex migrate:latest",
    "createseed": "knex seed:make init_seed",
    "runSeed": "knex seed:run",
    "rollback": "NODE_ENV=test knex migrate:rollback ",
    "test": "(dropdb --if-exists test-dibbzdb && createdb test-dibbzdb) &&  NODE_ENV=test mocha"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "knex": "^0.21.1",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}